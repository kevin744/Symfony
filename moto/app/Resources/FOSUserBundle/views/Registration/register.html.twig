<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Log in</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="{{ asset('bundles/app/bootstrap/css/bootstrap.min.css') }}">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="{{ asset('bundles/app/css/AdminLTE.min.css') }}">
    <!-- iCheck -->
    <link rel="stylesheet" href="{{ asset('bundles/app/plugins/iCheck/square/blue.css') }}">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition login-page">

<div class="login-box">


    {% if app.request.hasPreviousSession %}
        {% for type, messages in app.session.flashBag.all %}
            {% for message in messages %}
                <div class="{{ type }}">
                    {{ message|trans({}, 'FOSUserBundle') }}
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}

    <div class="login-logo">
        <a href="{{ path('homepage') }}"><b>Eurasian</b>Racing</a>
    </div>
    <!-- /.login-logo -->
    <div class="login-box-body">
        <p class="login-box-msg">Register to start your session</p>



        <form name="fos_user_registration_form" method="post" action="{{ path('fos_user_registration_register') }}" class="fos_user_registration_register" _lpchecked="1">
            <div id="fos_user_registration_form">
                {{ form_widget(form._token) }}
                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_email" class="required">Email</label>
                    <input class="form-control" type="email" id="fos_user_registration_form_email" name="fos_user_registration_form[email]" required="required">
                </div>
                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_username" class="required">Username</label>
                    <input class="form-control" type="text" id="fos_user_registration_form_username" name="fos_user_registration_form[username]" required="required" maxlength="180" pattern=".{2,}">
                </div>
                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_plainPassword_first" class="required">Password</label>
                    <input class="form-control" type="password" id="fos_user_registration_form_plainPassword_first" name="fos_user_registration_form[plainPassword][first]" required="required" autocomplete="new-password">
                </div>
                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_plainPassword_second" class="required">Repeat password</label>
                    <input class="form-control" type="password" id="fos_user_registration_form_plainPassword_second" name="fos_user_registration_form[plainPassword][second]" required="required" autocomplete="new-password">
                </div>
                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_firstname" class="required">Firstname</label>
                    <input class="form-control" type="text" id="fos_user_registration_form_firstname" name="fos_user_registration_form[firstname]" required="required" maxlength="255">
                </div>
                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_lastname" class="required">Lastname</label>
                    <input class="form-control" type="text" id="fos_user_registration_form_lastname" name="fos_user_registration_form[lastname]" required="required" maxlength="255">
                </div>

                <div>
                    <label class="required">Dob</label><div id="fos_user_registration_form_dob" maxlength="255">
                        <input class="form-control" type="date" name="fos_user_registration_form[dob]" id="fos_user_registration_form_dob">
                    </div>
                </div>

                <div class="form-group has-feedback">
                    <label for="fos_user_registration_form_job" class="required">Job</label>
                    <input class="form-control" type="text" id="fos_user_registration_form_job" name="fos_user_registration_form[job]" required="required" maxlength="255">
                </div>

                <div>
                    <input class="btn btn-primary btn-block btn-flat" type="submit" value="Register">
                </div>
            </div>
        </form>

    </div>
    <!-- /.login-box-body -->
</div>
<!-- /.login-box -->

<!-- jQuery 2.2.3 -->
<script src="{{ asset('bundles/app/plugins/jquery/jquery.min.js') }}"></script>
<!-- Bootstrap 4 -->
<script src="{{ asset('bundles/app/bootstrap/js/bootstrap.min.js') }}"></script>
<!-- iCheck -->
<script src="{{ asset('bundles/app/plugins/iCheck/icheck.min.js') }}"></script>
<script>
    $(function () {
        $('input').iCheck({
            checkboxClass: 'icheckbox_square-blue',
            radioClass: 'iradio_square-blue',
            increaseArea: '20%' // optional
        });
    });
</script>


</body>
</html>